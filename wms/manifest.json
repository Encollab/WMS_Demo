{
    "_version": "1.1.0",
    "sap.app": {
        "_version": "1.1.0",
        "id": "yelcho.wmsdemo",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "ach": "CA-UI5-DOC",
        "dataSources": {
            "wms_srv": {
                "uri": "/sap/opu/odata/sap/YWMS_SRV/",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0",
                    "defaultCountMode": "Inline"
                }
            }
        }
    },
    "sap.ui": {
        "_version": "1.1.0",
        "technology": "UI5",
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        },
        "supportedThemes": [
            "sap_bluecrystal"
        ]
    },
    "sap.ui5": {
        "_version": "1.1.0",
        "rootView": "yelcho.wmsdemo.App",
        "dependencies": {
            "minUI5Version": "1.34",
            "libs": {
                "sap.m": {}
            }
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "yelcho.wmsdemo.i18n.i18n"
                }
            },
            "": {
                "dataSource": "wms_srv"
            }
        },
        "resources": {
            "css": [{
                "uri": "css/style.css"
            }]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "viewPath": "yelcho.wmsdemo",
                "controlId": "app",
                "controlAggregation": "pages"
            },
            "routes": [{
                "pattern": "",
                "name": "default",
                "target": "dashboard"
            }, {
                "pattern": "bin",
                "name": "bin",
                "target": "binEnquiry"
            }, {
                "pattern": "bin/{binPath}",
                "name": "bindetail",
                "target": "binDetail"
            }],
            "targets": {
                "dashboard": {
                    "viewName": "Dashboard"
                },
                "binEnquiry": {
                    "viewName": "bin.Enquiry"
                },
                "binDetail": {
                    "viewName": "bin.Detail"
                },
                "objectNotFound": {
                    "viewName": "ObjectNotFound",
                    "viewId": "objectNotFound"
                }
            }
        }
    }
}